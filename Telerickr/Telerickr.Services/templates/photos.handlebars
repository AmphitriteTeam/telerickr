<div class="row">
    {{#each this}}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail photo-box" data-id="{{Id}}" data-title="{{Title}}">
            <h3 class="text-center">{{Title}}</h3>
            <img src="{{ImageUrl}}" title="{{Title}}" />
        </div>
    </div>
    {{/each}}
</div>