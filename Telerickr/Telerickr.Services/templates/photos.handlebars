<div class="row">
    {{#each this}}
    <div class="col-sm-4 portfolio-item">
        <div class="portfolio-link album-box" data-toggle="modal" data-id="{{Id}}" data-title="{{Title}}">
            <div class="caption">
                <div class="caption-content">
                    <i class="fa fa-search-plus fa-3x"></i>
                </div>
            </div>
            <h3 class="text-center">{{Title}}</h3>
            <a href="/#/photos/{{Id}}"><img src="{{ImageUrl}}" title="{{Title}}" class="img-responsive" /></a>
        </div>
    </div>
    {{/each}}
</div>